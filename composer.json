{
    "name": "label305/auja",
    "type": "library",
    "description": "An API wrapper for communicating with the Auja javascript frontend. Auja is an easy-to-use, easy-to-implement admin interface for managing your data.",
    "keywords": ["Auja", "PHP", "Admin"],
    "homepage": "http://label305.github.io/Auja",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Niek Haarman",
            "email": "niek@label305.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Label305/Auja-PHP/issues",
        "wiki": "https://github.com/Label305/Auja-PHP/wiki",
        "source": "https://github.com/Label305/Auja-PHP"
    },
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "henrikbjorn/phpspec-code-coverage": "~0.2",
        "satooshi/php-coveralls": "~0.6"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "minimum-stability": "stable"
}
